    "c8": "^7.7.1",
    "chai": "^4.3.4",
    "dirty-chai": "^2.0.1",
    "express": "^4.15.3",
    "jsdoc-to-markdown": "^7.0.1",
    "mocha": "^8.3.2",
    "semistandard": "*",
    "supertest": "^6.1.3"
  },
  "keywords": [
    "express",
    "log",
